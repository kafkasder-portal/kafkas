    "framer-motion": "^12.23.12",
    "i18next": "^23.10.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "lucide-react": "^0.330.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.0",
    "react-router-dom": "^6.30.1",
